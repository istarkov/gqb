{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "no-nested-ternary": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "no-sequences": 1,
    "comma-dangle": 0,
    "no-mixed-operators": 0,
    "no-underscore-dangle": 0,
    "no-restricted-properties": 0,
    "jsx-a11y/no-static-element-interactions": 0
  },
  "globals": {
    "ReactElement": false
  }
}
